host PrintString();
host PrintNewline();

function print(s)
{
    PrintString(s);
}

function println()
{
    PrintNewline();
}

function max(a, b)
{
    if(a > b)
    {
        return a;
    }
    else
    {
        return b;
    }
}

function sum(a, b)
{
    return a + b;
}

function draw_rectangle(_x, _y)
{
    var x;
    var y;

    y = 1;
    while(y <= _y)
    {
        x = 1;
        while(x <= _x)
        {
            var is_print;
            is_print = false;

            if(y == 1)
            {
                is_print = true;
            }
            if(y == _y)
            {
                is_print = true;
            }
            if(x == 1)
            {
                is_print = true;
            }
            if(x == _x)
            {
                is_print = true;
            }

            if(is_print == true)
            {
                print("*");
            }
            else
            {
                print(" ");
            }
            x += 1;
        }
        println();
        y += 1;
    }
}

function main()
{
    draw_rectangle(20, 10);

    print("Hello World~!");
    println();

    var a;
    var b;
    var c;
    
    a = 1;
    b = 100;
    c = max(a, b);

    var s;
    s = "The maximum between a=" $ a $ " and b=" $ b $ " is " $ c;
    print(s);
    println();
}

